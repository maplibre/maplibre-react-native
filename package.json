{
  "name": "@maplibre/maplibre-react-native",
  "description": "React Native library for creating maps with MapLibre Native for Android & iOS",
  "version": "11.0.0-alpha.5",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "author": "MapLibre",
  "keywords": [
    "maps",
    "ios",
    "android",
    "maplibre",
    "react-native",
    "mapbox"
  ],
  "license": "MIT",
  "homepage": "https://github.com/maplibre/maplibre-react-native#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maplibre/maplibre-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/maplibre/maplibre-react-native/issues"
  },
  "scripts": {
    "codegen": "tsx scripts/codegen.ts",
    "lint": "yarn lint:tsc && yarn lint:eslint",
    "lint:tsc": "tsc --project ./ && tsc --project ./examples/shared && yarn docs lint:tsc",
    "lint:eslint": "eslint ./ --max-warnings 0",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "test": "jest",
    "prepack": "bob build",
    "docs": "yarn workspace docs",
    "examples:react-native": "yarn workspace react-native-app",
    "examples:expo": "yarn workspace expo-app"
  },
  "workspaces": [
    "docs",
    "examples/*"
  ],
  "packageManager": "yarn@4.10.3",
  "main": "./lib/commonjs/index.js",
  "source": "./src/index.ts",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./package.json": "./package.json",
    "./app.plugin.js": {
      "require": {
        "default": "./app.plugin.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./lib/typescript/module/src/plugin/withMapLibre.d.ts",
        "default": "./lib/module/plugin/withMapLibre.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/plugin/withMapLibre.d.ts",
        "default": "./lib/commonjs/plugin/withMapLibre.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "app.plugin.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "MapLibreReactNativeSpec",
    "type": "all",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "org.maplibre.reactnative"
    },
    "ios": {
      "componentProvider": {
        "MLRNMapView": "MLRNMapViewComponentView",
        "MLRNCamera": "MLRNCameraComponentView"
      },
      "modulesProvider": {
        "MLRNMapViewModule": "MLRNMapViewModule"
      }
    }
  },
  "peerDependencies": {
    "@expo/config-plugins": ">=54.0.0",
    "@types/geojson": "^7946.0.0",
    "@types/react": ">=19.1.0",
    "react": ">=19.1.0",
    "react-native": ">=0.80.0"
  },
  "peerDependenciesMeta": {
    "@expo/config-plugins": {
      "optional": true
    },
    "@types/geojson": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@turf/distance": "^7.1.0",
    "@turf/helpers": "^7.1.0",
    "@turf/length": "^7.1.0",
    "@turf/nearest-point-on-line": "^7.1.0"
  },
  "devDependencies": {
    "@expo/config-plugins": "~54.0.1",
    "@maplibre/maplibre-gl-style-spec": "23.1.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/babel-preset": "^0.81.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sinonjs/fake-timers": "^15.0.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/ejs": "^3.1.5",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "documentation": "^14.0.0",
    "ejs": "^3.1.10",
    "eslint": "^8.57.1",
    "eslint-config-universe": "15.0.3",
    "expo-module-scripts": "^5.0.7",
    "jest": "^30.1.3",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "react-docgen": "^8.0.1",
    "react-native": "0.81.4",
    "react-native-builder-bob": "^0.40.13",
    "react-test-renderer": "19.1.0",
    "semantic-release": "^24.2.9",
    "snapshot-diff": "^0.10.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
